<!-- routing dynamic-message-routing cluster __CLUSTER__ link __LINK__ shutdown -->
<rpc semp-version="soltr/9_0VMR">
    <routing>
        <dynamic-message-routing>
            <cluster>
                <cluster-name>__CLUSTER__</cluster-name>
                <link>
                    <remote-node-name>__LINK__</remote-node-name>
                    <shutdown></shutdown>
                </link>
            </cluster>
        </dynamic-message-routing>
    </routing>
</rpc>
